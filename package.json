{
  "name": "occam-tarjan",
  "author": "James Smith",
  "version": "2.0.20",
  "license": "MIT, Anti-996",
  "homepage": "https://github.com/@jecs-imperial/occam-tarjan",
  "description": "An implementation of Tarjan's algoirthm.",
  "repository": {
    "type": "git",
    "url": "https://github.com/@jecs-imperial/occam-tarjan"
  },
  "dependencies": {
    "necessary": "^1.7.5"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.6",
    "watchful-cli": "^1.0.10"
  },
  "scripts": {
    "clean": "rm -rf ./lib",
    "watchful": "watchful --source-directory=./es6 --lib-directory=./lib",
    "batch": "npm run watchful batch --",
    "batch-debug": "npm run watchful batch -- --debug",
    "incremental": "npm run watchful incremental --",
    "incremental-debug": "npm run watchful incremental -- --debug",
    "build": "npm run clean && npm run batch",
    "build-debug": "npm run clean && npm run batch-debug",
    "watch": "npm run clean && npm run batch && npm run incremental",
    "watch-debug": "npm run clean && npm run batch-debug && npm run incremental-debug"
  }
}
